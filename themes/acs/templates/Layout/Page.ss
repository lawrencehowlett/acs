		<h1 class="page-title">$Title</h1>
		$Description
	</div>
</section>

<section class="page-section shade contact">
	<div class="inside">
		<div class="cols">
			<div class="col col2 contact-content">
				$Content
				$Form
			</div>
		</div>
	</div>
</section>